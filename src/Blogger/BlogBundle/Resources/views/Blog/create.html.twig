{% extends 'BloggerBlogBundle::layout.html.twig' %}
{% block body %}
<form action="{{ path('BloggerBlogBundle_blog_createBlog') }}" method="post" {{ form_enctype(form) }} class="blogger">
    {{ form_errors(form) }}

    {{ form_row(form.title) }}
    {{ form_row(form.blog) }}
    {{ form_row(form.tags) }}
    {{ form_row(form.uploadedFiles) }}

    {{ form_rest(form) }}

    <input type="submit" value="Add blog"/>
</form>
{% endblock %}